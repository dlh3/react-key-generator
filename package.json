{
  "name": "react-key-generator",
  "version": "1.0.0",
  "description": "A simple key extractor/generator, intended for use with arrays of objects in React",
  "author": "Dave Hughes",
  "license": "MIT",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "dependencies": {
    "@types/uuid": "^8.0.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.3",
    "@types/node": "^13.11.1",
    "gts": "^2.0.2",
    "jest": "^26.1.0",
    "jest-extended": "^0.11.5",
    "ts-jest": "^26.1.1",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "start": "tsc -w",
    "debug": "npm run compile && echo 'Open chrome://inspect in Chrome' && node --inspect-brk build/src/index.js",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check"
  },
  "keywords": [
    "react",
    "keys",
    "react-key",
    "key-generator"
  ],
  "homepage": "https://github.com/dlh3/react-key-generator#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dlh3/react-key-generator.git"
  },
  "bugs": {
    "url": "https://github.com/dlh3/react-key-generator/issues"
  }
}
